{
  "name": "create-expressts",
  "version": "0.1.0",
  "description": "CLI wizard to create Express.js projects with TypeScript",
  "type": "module",
  "bin": {
    "create-expressts": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc && npm run copy-templates",
    "copy-templates": "cp -r src/templates dist/",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "express",
    "typescript",
    "cli",
    "generator",
    "scaffold",
    "create"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@clack/prompts": "^0.8.2",
    "execa": "^9.5.2",
    "fs-extra": "^11.2.0",
    "kleur": "^4.1.5"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.10.5",
    "typescript": "^5.7.3",
    "tsx": "^4.19.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
